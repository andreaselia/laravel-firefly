{
    "name": "andreaselia/laravel-firefly",
    "description": "A simple forum package for Laravel.",
    "keywords": [
        "laravel",
        "forum",
        "discussion",
        "firefly"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Elia",
            "email": "andreaselia@live.co.uk"
        },
        {
            "name": "Tom Irons",
            "email": "tom.irons@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^8.0|^9.0|^10.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/routing": "^8.0|^9.0|^10.0",
        "spatie/laravel-sluggable": "^3.4.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Firefly\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firefly\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firefly\\FireflyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
